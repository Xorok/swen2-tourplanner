<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<HBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.14-internal"
      fx:controller="at.tiefenbrunner.swen2semesterprojekt.view.SearchBarView">
    <Button fx:id="showAllButton" onAction="#onShowAll" text="Show All"/>
    <TextField fx:id="searchField" onAction="#onSearch" HBox.hgrow="ALWAYS"/>
    <Button fx:id="searchButton" onAction="#onSearch" text="Search &#x1F50E;"/>
</HBox>